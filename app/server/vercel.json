{
    "version": 2,
    "buildCommand": "pnpm build",
    "installCommand": "pnpm install",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}